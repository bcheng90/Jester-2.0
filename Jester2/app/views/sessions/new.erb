<div class="container">
  <h2 class="center">What do we call you, Jester?</h2>
  <%= form_tag '/login' do %>
    Jester Name: <%= text_field_tag :name %>
    Password: <%= password_field_tag :password %>
    <%= submit_tag "Submit" %>
  <% end %>
  <p>No account? <%= link_to "Sign up!", signup_path%></p>
</div>